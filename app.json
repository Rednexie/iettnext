{
  "expo": {
    "name": "iettnext",
    "slug": "iettnext",
    "version": "0.0.8",
    "orientation": "portrait",
    "icon": "./assets/images/iettnext.png",
    "scheme": "iettnext",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/iettnext.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.rednexie.iettnext"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/iettnext.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/iettnext.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-font",
      "expo-asset"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {}
      }
    }
  }
}
